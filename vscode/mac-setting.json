{
    "editor.fontFamily": "IntelOne Mono",
    // "editor.fontLigatures": "ss01",
    "editor.fontSize": 16,
    "editor.glyphMargin": false,
    "editor.minimap.enabled": false,
    "editor.wordBasedSuggestions": "off",
    "editor.wordSeparators": "`~!@#$%^&*()-=+[{]}\\|;:'\",.<>/?（），。、",
    "files.insertFinalNewline": true,
    "explorer.compactFolders": false,
    "files.trimTrailingWhitespace": true,
    "telemetry.telemetryLevel": "crash",
    "rust-analyzer.imports.granularity.group": "module",
    "rust-analyzer.lens.enable": false,
    // "editor.semanticHighlighting.enabled": true,
    "rust-analyzer.diagnostics.experimental.enable": false,
    // "rust-analyzer.checkOnSave.enable": false,
    // "rust-analyzer.diagnostics.enable": false,
    // "rust-analyzer.semanticHighlighting.strings.enable": false,
    "rust-analyzer.server.path": "~/.rustup/toolchains/nightly-x86_64-apple-darwin/bin/rust-analyzer",
    "go.gopath": "/Users/vizee/src/go",
    "go.toolsGopath": "/Users/vizee/src/go",
    "go.testFlags": [
        "-v"
    ],
    "go.lintOnSave": "off",
    "go.vetOnSave": "off",
    "go.useLanguageServer": true,
    "gopls": {
        "ui.completion.usePlaceholders": false
    },
    "go.toolsManagement.autoUpdate": false,
    "markdown.preview.doubleClickToSwitchToEditor": false,
    // "nasc-touchbar.selectNext": false,
    // "nasc-touchbar.togglePanel": false,
    // "nasc-touchbar.addCursorBelow": false,
    // "nasc-touchbar.showReferences": true,
    // "nasc-touchbar.peekDefinition": true,
    "workbench.startupEditor": "welcomePageInEmptyWorkbench",
    "window.restoreWindows": "none",
    "extensions.ignoreRecommendations": true,
    "remote.SSH.localServerDownload": "off",
    "remote.SSH.remotePlatform": {
        "q.clouddev": "linux"
    },
    "workbench.sideBar.location": "left",
    "editor.folding": false,
    "git.enabled": true,
    "[rust]": {
        "editor.formatOnSave": true,
        "editor.inlayHints.enabled": "off",
        "editor.codeLens": true
        // "editor.codeActionsOnSave": {
        //     "source.organizeImports": true
        // },
        // "editor.snippetSuggestions": "none",
    },
    "[go]": {
        "editor.formatOnSave": true,
        "editor.codeActionsOnSave": {
            "source.organizeImports": "explicit"
        },
        "editor.codeLens": true,
        "editor.inlayHints.enabled": "off",
    },
    "[cpp]": {
        "editor.wordBasedSuggestions": "currentDocument",
        "editor.suggest.snippetsPreventQuickSuggestions": false
    },
    "[makefile]": {
        "editor.insertSpaces": false,
    },
    "[markdown]": {
        "editor.unicodeHighlight.ambiguousCharacters": false,
        "editor.unicodeHighlight.invisibleCharacters": false,
        "editor.wordWrap": "on",
        "editor.quickSuggestions": {
            "comments": "off",
            "strings": "off",
            "other": "off"
        },
    },
    "[proto]": {
        "editor.wordBasedSuggestions": "currentDocument",
    },
    "terminal.integrated.fontSize": 14,
    "diffEditor.ignoreTrimWhitespace": false,
    "editor.inlineSuggest.enabled": true,
    "breadcrumbs.enabled": false,
    "editor.guides.bracketPairs": "active",
    "terminal.integrated.cwd": "${fileDirname}",
    "editor.unicodeHighlight.allowedLocales": {
        "zh-hans": true,
        "zh-hant": true
    },
    "editor.language.colorizedBracketPairs": [],
    "editor.semanticHighlighting.enabled": true,
    "git.confirmSync": false,
    "workbench.layoutControl.enabled": false,
    "workbench.iconTheme": "vs-minimal",
    "editor.unicodeHighlight.nonBasicASCII": false,
    "redhat.telemetry.enabled": false,
    "extensions.experimental.affinity": {
        "asvetliakov.vscode-neovim": 1
    },
    "window.commandCenter": false,
    "workbench.activityBar.location": "hidden",
    "[cpp][proto]": {
        "editor.wordBasedSuggestions": "matchingDocuments"
    }
}
